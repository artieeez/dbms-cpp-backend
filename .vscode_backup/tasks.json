        "${workspaceFolder}\\src\\index",
        "/I",
        "${workspaceFolder}\\src\\controller",
        "${workspaceFolder}\\src\\database\\context.cpp",
        "${workspaceFolder}\\src\\controller\\linearSearchController.cpp",
        "${workspaceFolder}\\src\\test\\test_controller_linear.cpp",
        "/Fo:${workspaceFolder}\\build\\obj\\",
        "/Fd:${workspaceFolder}\\build\\obj\\",
        "/Fe:${workspaceFolder}\\build\\test_controller_linear.exe",
      ],
      "problemMatcher": ["$msCompile"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "reveal": "always",
        "panel": "shared",
        "clear": true
      }
    },
    {
